# Court Client Environment Configuration
# Copy this file to .env and modify the values according to your setup

# Court Identification
COURT_ID=court-001
AUTH_TOKEN=dev-token-1

# Feed Starter Service Connection
SERVER_HOST=localhost
SERVER_PORT=3000

# Camera/RTSP Configuration
RTSP_URL=rtsp://admin:12345qwe@192.168.1.7:554/Streaming/Channels/101

# Streaming Configuration (Optional)
YOUTUBE_STREAM_KEY=your-youtube-stream-key-here

# Local Storage
RECORDINGS_DIR=recordings

# Logging and Monitoring
LOG_LEVEL=INFO
HEARTBEAT_INTERVAL=15

# Connection Retry Settings
MAX_RETRIES=5
RETRY_DELAY=5
