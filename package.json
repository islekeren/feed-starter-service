{
  "name": "feed-starter-service",
  "version": "1.0.0",
  "description": "WebSocket microservice bridging mobile app backend with court edge PCs",
  "type": "module",
  "main": "src/app.js",
  "scripts": {
    "dev": "nodemon --experimental-modules src/app.js",
    "start": "node src/app.js",
    "test": "set NODE_OPTIONS=--experimental-vm-modules && set NODE_ENV=test && npx jest",
    "test:watch": "npm test -- --watch",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "format": "prettier --write .",
    "demo": "node demo.js",
    "prepare": "husky install"
  },
  "keywords": [
    "websocket",
    "streaming",
    "microservice",
    "court",
    "tennis"
  ],
  "author": "Tennis Stream Service",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-pino-logger": "^7.0.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "node-fetch": "^3.3.2",
    "pino": "^8.17.2",
    "pino-pretty": "^13.1.1",
    "uuid": "^9.0.1",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.4"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "transform": {}
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
